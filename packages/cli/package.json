    "@babel/types": "^7.24.6",
    "commander": "^12.1.0",
    "exifr": "^7.1.3",
    "prettier": "^3.2.5"
  },
  "devDependencies": {
    "microbundle": "^0.15.1",
    "@types/babel__generator": "^7.6.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
